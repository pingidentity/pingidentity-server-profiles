    --publisher-name 'Debug Trace Logger'  \
    --set enabled:true 

dsconfig create-identity-mapper \
    --mapper-name 'User ID Identity Mapper'  \
    --type exact-match  \
    --set enabled:true  \
    --set match-attribute:cn  \
    --set match-attribute:entryUUID  \
    --set match-attribute:uid  \
    --set match-base-dn:cn=config  \
    --set 'match-base-dn:ou=people,${USER_BASE_DN}'

dsconfig create-access-token-validator \
    --validator-name jwt-atv  \
    --type jwt  \
    --set 'identity-mapper:User ID Identity Mapper'  \
    --set 'description:Validates signed JWTs that I create myself'  \
    --set enabled:true  \
    --set evaluation-order-index:50  \
    --set 'signing-certificate:My Cert' 

dsconfig create-token-resource-lookup-method \
    --validator-name jwt-atv  \
    --method-name 'User by uid'  \
    --type scim  \
    --set evaluation-order-index:1000  \
    --set scim-resource-type:Users  \
    --set 'match-filter:uid eq "%sub%"' 

